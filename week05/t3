#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	int num;
	cout << "Enter a number-" << ' ';
	cin >> num;

	if (num < 0 || num>10)
		cout << "Enter a number between 0-10" << endl;
	else
		for (int i = num; i < 100; i++) {
			if (i % 2 == 0 && i % 3 == 0)
				cout << i << ' ';
		}

	system("PAUSE");
    return 0;
}
